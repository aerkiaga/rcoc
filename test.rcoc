/* This is an example file to test rcoc */

// basic syntax
let proof_a:
    @(T: Prop, x: T) T
=
    |T: Prop, x: T| {|y: T| y}(x)
;

// -> operator
let proof_b:
    @(T, P: Prop) T -> {T -> P} -> P
=
    |T, P: Prop, x: T, y: T -> P| y(x)
;

// exists() operator
let proof_c:
    @(T: Prop) exists(P: Prop) T -> P
=
    |T, Q: Prop|
    |h: @(P: Prop) {T -> P} -> Q|
    h(T)(|x: T| x)
;

// /\ operator
let proof_d:
    @(A, B: Prop) A /\ B -> A
=
    |A, B: Prop|
    |h: @(P: Prop) {A -> B -> P} -> P|
    h(A)(|x: A, y: B| x)
;
